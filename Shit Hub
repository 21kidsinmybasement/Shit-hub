local SettingsTab = Window:CreateTab("üé¨ setting", nil) -- Title, Image
local PlayerSection = SettingsTab:CreateSection("Setting")

local Slider = SettingsTab:CreateSlider({
   Name = "Walkspeed slider",
   Range = {0, 300},
   Increment = 1,
   Suffix = "WS",
   CurrentValue = 16,
   Flag = "Slider1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
game.Workspace.Players.LocalPlayer.Humanoid.Walkspeed = (Value)
   end,
})

local Slider = SettingsTab:CreateSlider({
